{
    "name": "pawel-brzezinski/smart-image-bundle",
    "description": "Symfony bundle that give an easy possibility to use responsive image services.",
    "type": "symofny-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Brzeziński",
            "email": "pawel.brzezinski@smartint.pl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PB\\Bundle\\SmartImageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PB\\Bundle\\SmartImageBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "symfony/http-kernel": "^3.4|^4.0|^5.0",
        "twig/twig": "^2.4.4|^3.0",
        "beberlei/assert": "^3.2",
        "symfony/dependency-injection": "^3.4|^4.0|^5.0",
        "symfony/config": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^8.0"
    }
}
